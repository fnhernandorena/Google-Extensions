{
    "manifest_version": 3,
    "name": "Image Gallery",
    "version": "1.0",
    "description": "Una extensión de Chrome para mostrar todas las imágenes de una página.",
    "permissions": ["activeTab", "storage", "tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "128": "images/icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  